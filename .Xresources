*font: -xos4-terminus-medium-r-normal-*-18-*-72-72-c-100-*-*
XTerm*background: #000000
XTerm*foreground: #FFFFFF
XTerm*color4: #0000FF
XTerm*color12: #0000FF
URxvt.background: #000000
URxvt.foreground: #FFFFFF
URxvt.scrollBar: false
URxvt.cursorBlink: true

URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select,resize-font

URxvt.matcher.button: 1
URxvt.url-select.underline:true

URxvt.url-launcher: /usr/bin/qutebrowser

URxvt.keysym.C-j: resize-font:smaller
URxvt.keysym.C-k: resize-font:bigger
